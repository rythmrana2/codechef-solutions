#include <stdio.h>

int main(void) {
	int t,n,x;
	int a[1000];
	scanf("%d",&t);
	int flag=0;
	for(int i=0;i<t;i++)
	{
		scanf("%d%d",&n,&x);
	
	
	      for(int j=0;j<n;j++)
		{
			scanf("%d",&a[j]);
			
			
		}
		for(int k=0;k<n;k++)
		{
			
			if(a[k]>=x)
			flag=1;
			
		}
		if(flag==1)
		printf("YES\n");
		else
		printf("NO\n");
		flag=0;
		
	}
	return 0;
}
